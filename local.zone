# /etc/unbound/unbound.conf.d/local_names.conf
private-address: 192.168.1.0/24
local-zone: "home.zetan.cc" static
        local-data: "gateway.home.zetan.cc IN A 192.168.1.1"
        local-data: "printer.home.zetan.cc IN A 192.168.1.2"
        local-data: "computer.home.zetan.cc IN A 192.168.1.3"
        local-data: "server.home.zetan.cc IN A 192.168.1.10"
        local-data-ptr: "192.168.1.1 gateway.home.zetan.cc"
        local-data-ptr: "192.168.1.2 printer.home.zetan.cc"
        local-data-ptr: "192.168.1.3 computer.home.zetan.cc"
        local-data-ptr: "192.168.1.10 server.home.zetan.cc"
