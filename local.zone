# /etc/unbound/unbound.conf.d/local_names.conf
private-address: 192.168.0.0/24
local-zone: "home.zetan.cc" static
        local-data: "gateway.home.zetan.cc IN A 192.168.0.1"
        local-data: "printer.home.zetan.cc IN A 192.168.0.2"
        local-data: "computer.home.zetan.cc IN A 192.168.0.3"
        local-data: "server.home.zetan.cc IN A 192.168.0.100"
        local-data-ptr: "192.168.0.1 gateway.home.zetan.cc"
        local-data-ptr: "192.168.0.2 printer.home.zetan.cc"
        local-data-ptr: "192.168.0.3 computer.home.zetan.cc"
        local-data-ptr: "192.168.0.100 server.home.zetan.cc"
